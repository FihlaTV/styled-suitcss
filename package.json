{
  "name": "styled-suitcss",
  "version": "1.0.0-aplha.3",
  "bin": {
    "extract-styled-suitcss": "tools/cli.js"
  },
  "main": "lib/server.js",
  "browser": "lib/client.js",
  "repository": "git@github.com:sinnerschrader/styled-ui.git",
  "author": "Gregor Adams <greg@pixelass.com>",
  "files": [
    "lib",
    "tools"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc && yarn build:ssr; yarn build:examples",
    "build:examples": "node tools/cli 'examples/src/**/*.js' -o examples/dist",
    "build:watch": "tsc --watch",
    "build:ssr": "node app/server.js",
    "clean": "yarn clean:lib; yarn clean:examples",
    "clean:lib": "rimraf lib",
    "clean:examples": "rimraf examples/dist",
    "dev": "yarn build && concurrently 'yarn build:watch' 'yarn start'",
    "format": "yarn format:md; yarn format:js; yarn format:ts; yarn format:css; yarn format:json",
    "format:json": "prettier package.json tsconfig.json .babelrc '{app,src,tools,examples/src}/**/*.json' --parser json --write",
    "format:css": "prettier '{app,src,tools,examples/src}/**/*.css' --parser postcss --write",
    "format:js": "prettier .prettierrc.js webpack.config.js '{app,src,tools,examples/src}/**/*.js' --parser babylon --write",
    "format:ts": "prettier '{app,src,tools,examples/src}/**/*.ts' --write",
    "format:md": "prettier Readme.md '{app,src,tools,examples/src}/**/*.md' --parser markdown --write",
    "gh-pages": "cross-env NODE_ENV=production yarn build && cross-env NODE_ENV=production webpack",
    "prebuild": "yarn clean",
    "release": "standard-version",
    "start": "webpack-dev-server --hot",
    "test": "ava 'test/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "pre-release": "yarn tsc"
    }
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-2": "^7.0.0-beta.46",
    "@types/classnames": "^2.2.3",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "ava": "^1.0.0-beta.4",
    "babel-loader": "^8.0.0-beta.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.5",
    "html-minifier": "^3.5.15",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "prettier": "^1.12.1",
    "standard-version": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "classnames": "^2.2.5",
    "cssbeautify": "^0.3.1",
    "decamelize": "^2.0.0",
    "globby": "^8.0.1",
    "mkdirp": "^0.5.1",
    "pify": "^3.0.0",
    "pretty": "^2.0.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "rimraf": "^2.6.2",
    "stylis": "^3.5.0",
    "tslib": "^1.9.0",
    "winston": "^3.0.0-rc5"
  }
}
